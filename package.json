{
	"name": "meshtastic-starter-app",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"dependencies": {
		"@chakra-ui/react": "^2.8.2",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@meshtastic/core": "npm:@jsr/meshtastic__core@^2.6.2",
		"@meshtastic/protobufs": "npm:@jsr/meshtastic__protobufs@^2.6.2",
		"@meshtastic/transport-deno": "npm:@jsr/meshtastic__transport-deno@^0.1.1",
		"@meshtastic/transport-http": "npm:@jsr/meshtastic__transport-http@^0.2.1",
		"@meshtastic/transport-web-bluetooth": "npm:@jsr/meshtastic__transport-web-bluetooth@^0.1.1",
		"@meshtastic/transport-web-serial": "npm:@jsr/meshtastic__transport-web-serial@^0.2.1",
		"better-sqlite3": "^11.10.0",
		"cors": "^2.8.5",
		"date-fns": "^2.30.0",
		"express": "^4.18.2",
		"framer-motion": "^10.16.4",
		"next-themes": "^0.4.6",
		"node-fetch": "^2.7.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^5.5.0",
		"react-router-dom": "^6.30.0",
		"react-scripts": "5.0.1",
		"rxjs": "^7.8.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"server": "node server/index.js",
		"dev": "concurrently \"npm run server\" \"npm run start\"",
		"docker:up": "docker compose up -d",
		"docker:down": "docker compose down",
		"docker:logs": "docker compose logs -f",
		"docker:restart": "docker compose restart",
		"docker:start": "docker compose up -d && npm run open",
		"open": "npx open-cli http://localhost:3000",
		"docker:multi-node": "docker compose -f docker-compose.multi-node.yaml up -d",
		"docker:multi-node:down": "docker compose -f docker-compose.multi-node.yaml down"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"concurrently": "^8.2.2"
	}
}
